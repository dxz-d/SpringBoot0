<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mqtt.mapper.DeviceMapper">

    <insert id="updateDeviceStatus" parameterType="com.mqtt.pojo.Device">
        insert into device(username, ts)
        values (#{username}, #{ts})
    </insert>
</mapper>
